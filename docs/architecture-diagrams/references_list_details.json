          },
      
          "Low_Entropy_Synthetic_Data": {
            "name": "Artificial Linear & Nonlinear Data Generation",
            "purpose": "Validate algorithms on controlled low-entropy scenarios.",
            "linear_data": {
              "formula": "y = w·X + ε",
              "generation_steps": [
                "Sample features X from Gaussian distribution.",
